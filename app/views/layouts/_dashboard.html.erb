<div class="bgadmin">
  <div class="adminhead">
    <div class="pull-right dashhead">
      <h2 class="myadmintxt">Admin Dashboard</h2> <br /><br />
      <span class="modalform"><%= render 'units/modaltest' %></span>
    </div> 

    <a href="/"><img class="downimg pull-left" src="/assets/pmlogo1.png" alt="logo"></a>

  </div>
  <div class="text-center">
    <div class="row listcol">
      <h2 class="textheadone">&nbsp;</h2>
      <div class="col-xs-1"></div>
      <div class="col-xs-8 dashtab">

        <div id="content">
          <ul id="tabs" class="nav nav-tabs" data-tabs="tabs">
            <li class="active"><a href="#availunits" data-toggle="tab">Listed Units</a></li>
            <li><a href="#allunits" data-toggle="tab">All Units</a></li>
            <li><a href="#rentalapps" data-toggle="tab">All </a></li>
            <li><a href="#requests" data-toggle="tab">Requests</a></li>
          </ul>
          <div id="my-tab-content" class="tab-content">
            <div class="tab-pane active" id="availunits">

              <div class="listingcol" id="firsttab">
                <ul class="listings">
                  <%= render 'layouts/dashlisting' %>
                  <%#= render 'layouts/unitlist' %>

                </div>
              </div>
              <div class="tab-pane" id="allunits">
                <br />
                <div class="text-center">
                  <h1>All Units:</h1>

                  <% @units.each do |u| %>
                  <ul class="unitslis"><li>Unit number:</li> 
                    <%= u.uNumber %>

                    <%= link_to 'Edit', edit_unit_path(u) %>
                    
                  </ul>
                  <% end %>
                </div>
              </div>
              <div class="tab-pane" id="rentalapps">
                <br />
                <%= render 'buildings/propertylist' %>
              </div>
              <div class="tab-pane" id="requests">
                <br />
                <h1>List of all incoming requests.</h1>
              </div>
            </div>
          </div>

        </div>